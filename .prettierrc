{
	"tabWidth": 2,
	"useTabs": true,
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"printWidth": 96,
	"proseWrap": "preserve",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"svelteSortOrder": "scripts-markup-styles",
	"svelteBracketNewLine": true
}
